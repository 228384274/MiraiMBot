<?xml version="1.0" encoding="UTF-8"?>
 <Configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT"><PatternLayout pattern="[%d{HH:mm:ss} %level] %msg%n"/>
        </Console>
    </Appenders>
    <Loggers>
        <Logger level="info" name="red.mohist"/>
        <Root level="all">
            <AppenderRef ref="Console"/>
        </Root>
    </Loggers>
</Configuration>